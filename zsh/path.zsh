# ~/.dotfiles/zsh/path.zsh

# Go binaries
export PATH="$HOME/go/bin:$PATH"

# Personal scripts
export PATH="$HOME/scripts:$PATH"

# Doom Emacs
export PATH="$HOME/.config/emacs/bin:$PATH"

# pipx
export PATH="$PATH:$HOME/.local/bin"

# Yarn global packages
export PATH="$PATH:$(yarn global bin)"

